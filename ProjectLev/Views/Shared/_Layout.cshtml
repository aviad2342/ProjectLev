<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />

    @if (IsSectionDefined("Home"))
    {
        @RenderSection("Home", required: false)
    }

    @if (IsSectionDefined("User"))
    {
        @RenderSection("User", required: false)
    }
    @if (IsSectionDefined("Customer"))
    {
        @RenderSection("Customer", required: false)
    }
    @if (IsSectionDefined("UserUpdate"))
    {
        @RenderSection("UserUpdate", required: false)
    }

</head>
<body class="hold-transition skin-blue sidebar-mini">
      




